                    si:hasLeftUnitTerm [ a si:UnitPower ;
                            si:hasNumericExponent "-1"^^xsd:short ;
                            si:hasUnitBase units:metre ] ;
                    si:hasRightUnitTerm [ a si:UnitPower ;
                            si:hasNumericExponent "-1"^^xsd:short ;
                            si:hasUnitBase units:kelvin ] ] ] .

quantities:TDIF a si:QuantityKind ;
